      <th scope="col">Description</th>
      <th scope="col">Payload</th>
    </tr>
  </thead>
  <tbody>
    {{#each events}}
      <tr>
        <td>{{this.service}}</td>
        <td id="{{this.name}}">{{this.name}}</td>
        <td>{{this.schemaDescription}}</td>
        <td id="{{this.name}}-payload" scope="row">
          <table>
            {{#each this.payload}}
            <tr><td>{{@key}}</td><td>{{this.type}}</td></tr>
            {{/each}}
          </table>
        </td>
      </tr>
    {{/each}}
  </tbody>
