{
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "eslint.nodePath": ".yarn/sdks",
  "prettier.prettierPath": ".yarn/sdks/prettier/index.js",
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "vsicons.presets.tsOfficial": true,
  "vsicons.associations.files": [
    { "icon": "json", "extensions": [".schemas.ts", ".schema.ts"] },
    {
      "icon": "Command",
      "extensions": [".commands.ts", ".command.ts"],
      "format": "svg"
    },
    {
      "icon": "Event",
      "extensions": [".events.ts", ".event.ts"],
      "format": "svg"
    },
    {
      "icon": "Aggregate",
      "extensions": [".aggregates.ts", ".aggregate.ts"],
      "format": "svg"
    },
    {
      "icon": "Policy",
      "extensions": [".policies.ts", ".policy.ts"],
      "format": "svg"
    },
    {
      "icon": "Projector",
      "extensions": [".projectors.ts", ".projector.ts"],
      "format": "svg"
    },
    {
      "icon": "sql",
      "extensions": [".models.ts", ".model.ts"],
      "format": "svg"
    }
  ],
  "vsicons.associations.folders": [
    { "icon": "Aggregate", "extensions": ["aggregates"], "format": "svg" },
    { "icon": "Policy", "extensions": ["policies"], "format": "svg" },
    { "icon": "Projector", "extensions": ["projectors"], "format": "svg" },
    { "icon": "json", "extensions": ["schemas"] }
  ]
}
